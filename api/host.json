{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "http": {
      "cors": {
        "allowedOrigins": [
          "http://127.0.0.1:5500",
          "http://localhost:5500"
        ],
        "supportCredentials": false
      }
    }
  }
}